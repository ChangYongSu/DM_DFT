        MENUITEM "VIEW\tF4",                    ID_EDIT_CURRENT
        MENUITEM "SYNTAX CHECK\tF2",            ID_EDIT_COMPILE
        MENUITEM SEPARATOR
        MENUITEM "Model Registration",          ID_MODEL_REGISTRATION
        MENUITEM SEPARATOR
        MENUITEM "RECENT FILES",                ID_FILE_MRU_FILE1, GRAYED
    END
    POPUP "RUN(&R)"
    BEGIN
        MENUITEM "RUN(&R)\tF5",                 ID_RUN_RUN
        MENUITEM "REF-IMAGE(&M)\tF6",           ID_RUN_MAKE_REF
        MENUITEM "REF-AUDIO(&A)\tF7",           ID_RUN_ADJ_SPEC
        MENUITEM "STOP(&S)\tEsc",               ID_RUN_ABORT
        MENUITEM SEPARATOR
        MENUITEM "NEXT(&N)\tF9",                ID_RUN_NEXT
        MENUITEM "PREVIOUS(&P)\tF10",           ID_RUN_PREV
        MENUITEM "JUMP(&J)\tF11",               ID_RUN_JUMP
        MENUITEM SEPARATOR
        POPUP "RUN MODE(&M)"
        BEGIN
            MENUITEM "AUTO(&A)",                    ID_RUN_CONTINUOUS, CHECKED
            MENUITEM "ERROR STOP",                  ID_RUN_ERRORABORT
            MENUITEM "STEP BY STEP(&S)",            ID_RUN_STEP
        END
        POPUP "START MODE"
        BEGIN
            MENUITEM "AUTO",                        ID_START_AUTO
            MENUITEM "MANUAL",                      ID_START_MANUAL
        END
        MENUITEM SEPARATOR
        MENUITEM "IMAGE GRAB START",            ID_GRAB_START
        MENUITEM "IMAGE GRAB STOP",             ID_GRAB_STOP
        MENUITEM "AUDIO MEASURE START",         ID_AUDIO_MEAS_START
        MENUITEM "AUDIO MEASURE STOP",          ID_AUDIO_MEAS_STOP
        MENUITEM SEPARATOR
        MENUITEM "MES-DATA UPLOAD",             ID_MES_DATA_UPLOAD
    END
    POPUP "ANALYSE(&A)"
    BEGIN
        MENUITEM "A/V Analysis(&A)",            ID_IMAGE_DIALOG
        MENUITEM "I2C-ADC Manual Check(&I)",    ID_I2C_MANUAL_CHECK_DIALOG, GRAYED
        MENUITEM "RS232C-ADC Manual Check(&R)", ID_RS232C_MANUAL_CHECK_DIALOG
        MENUITEM SEPARATOR
        MENUITEM "Audio Out_L Start",           ID_AUDIO_OUTL_START
        MENUITEM "Audio Out_L Stop",            ID_AUDIO_OUTL_STOP
        MENUITEM "Audio Out_R Start",           ID_AUDIO_OUTR_START
        MENUITEM "Audio Out_R Stop",            ID_AUDIO_OUTR_STOP
        MENUITEM SEPARATOR
        MENUITEM "Cylinder(JIG) Up",            ID_JIG_UP
    END
    POPUP "COMPOSE(&C)"
    BEGIN
        MENUITEM "Sequence Editor(&E)",         ID_SEQ_EDIT
    END
    POPUP "OPTION(&O)"
    BEGIN
        MENUITEM "&Option",                     ID_OPTION
        POPUP "JIG UP Option"
        BEGIN
            MENUITEM "GOOD Case",                   ID_JIGUP_GOOD
            MENUITEM "ALL Case",                    ID_JIGUP_ALL
            MENUITEM "None",                        ID_JIGUP_NOT
        END
    END
    POPUP "HELP(&H)"
    BEGIN
        MENUITEM "DFT SYSTEM(&A)...",           ID_APP_ABOUT
    END
    POPUP "EXIT(&X)"
    BEGIN
        MENUITEM "EXIT PROGRAM(&P)",            ID_APP_EXIT
    END
END


